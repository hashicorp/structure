<div class="docs-cta-link-states">
  <Pds::ButtonSet>
    {{!-- PRIMARY --}}
    <Docs::CtaLink
      @variant="primary"
      @disabled={{@disabled}}
      ...attributes
      data-test-visreg="cta-link-primary"
    >
      Primary
    </Docs::CtaLink>

    {{!-- SECONDARY --}}
    <Docs::CtaLink
      @disabled={{@disabled}}
      ...attributes
      data-test-visreg="cta-link-secondary"
    >
      Secondary
    </Docs::CtaLink>

    {{!-- WARNING --}}
    <Docs::CtaLink
      @variant="warning"
      @disabled={{@disabled}}
      ...attributes
      data-test-visreg="cta-link-warning"
    >
      Warning
    </Docs::CtaLink>

    {{!-- GHOST --}}
    <Docs::CtaLink
      @variant="ghost"
      @disabled={{@disabled}}
      ...attributes
      data-test-visreg="cta-link-ghost"
    >
